{
  "name": "ludendorff",
  "version": "0.1.0",
  "author": "Isaiah Collins Abetong <isaiahcollins_02@live.com> (https://github.com/icabetong)",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@firebase/firestore-types": "^2.3.0",
    "@heroicons/react": "^1.0.3",
    "@material-ui/core": "^4.12.3",
    "@material-ui/data-grid": "^4.0.0-alpha.35",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.16",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "electron-is-dev": "^2.0.0",
    "date-fns": "^2.23.0",
    "firebase": "^8.9.0",
    "i18next": "^20.4.0",
    "install": "^0.13.0",
    "jsqr": "^1.4.0",
    "notistack": "^1.0.10",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.4",
    "react-lazy-load-image-component": "^1.5.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "main": "src/starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "./node_modules/.bin/electron-builder -c.extraMetadata.main=build/starter.js",
    "preelectron-package": "npm run build"
  },
  "build": {
    "appId": "io.capstone.ludendorff",
    "dmg": {
      "contents": [
        {
          "x": 100,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "win": {
      "target": "squirrel"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.8",
    "concurrently": "^6.2.0",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "file-loader": "^6.1.1",
    "wait-on": "^6.0.0"
  }
}
