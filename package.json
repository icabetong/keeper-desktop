{
  "name": "ludendorff",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@date-io/moment": "^2.11.0",
    "@firebase/firestore-types": "^2.5.0",
    "@heroicons/react": "^1.0.5",
    "@material-ui/core": "^4.12.3",
    "@material-ui/data-grid": "^4.0.0-alpha.35",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.36",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-instantsearch-core": "^6.10.5",
    "@types/react-instantsearch-dom": "^6.12.0",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "algoliasearch": "^4.11.0",
    "axios": "^0.21.4",
    "clsx": "^1.1.1",
    "electron-is-dev": "^2.0.0",
    "firebase": "^8.1.0",
    "i18next": "^21.5.4",
    "install": "^0.13.0",
    "jspdf": "^2.4.0",
    "jsqr": "^1.4.0",
    "notistack": "^1.0.10",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.18.0",
    "react-i18next": "^11.13.0",
    "react-instantsearch-dom": "^6.15.0",
    "react-lazy-load-image-component": "^1.5.1",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.4.4",
    "web-vitals": "^1.1.2",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "main": "starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . ludendorff --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Ludendorff\"",
    "package-linux": "electron-packager . ludendorff --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "build": {
    "appId": "io.capstone.ludendorff",
    "productName": "Ludendorff",
    "dmg": {
      "contents": [
        {
          "x": 100,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "./public/desktop/mac/icon.icns",
      "category": "public.app-category.utilities"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "icon": "./public/desktop/png/256x256.png"
    },
    "win": {
      "target": "squirrel",
      "icon": "./public/desktop/win/icon.ico"
    },
    "directories": {
      "buildResources": "build",
      "app": "build"
    },
    "files": [
      "./public/**/*"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.8",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "electron": "^13.1.7",
    "electron-icon-builder": "^2.0.1",
    "electron-packager": "^15.4.0",
    "file-loader": "^6.1.1",
    "wait-on": "^6.0.0"
  }
}
