{
  "name": "ludendorff",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "author": "Isaiah Collins Abetong",
  "description": "CLIRDEC Inventory System",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@firebase/firestore-types": "^2.5.0",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.80",
    "@mui/material": "^5.6.4",
    "@mui/styles": "^5.6.2",
    "@mui/x-data-grid": "^5.10.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/node": "^12.20.50",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.16",
    "@types/react-instantsearch-core": "^6.10.8",
    "@types/react-instantsearch-dom": "^6.12.3",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "algoliasearch": "^4.13.0",
    "axios": "^0.21.4",
    "date-fns": "^2.28.0",
    "exceljs": "^4.3.0",
    "firebase": "^9.7.0",
    "i18next": "^20.6.1",
    "notistack": "^2.0.4",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "7.29.0",
    "react-i18next": "^11.16.7",
    "react-instantsearch-dom": "^6.24.1",
    "react-lazy-load-image-component": "^1.5.4",
    "react-router": "^5.3.1",
    "react-router-dom": "^5.3.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.6.4",
    "use-pagination-firestore": "^0.6.0",
    "web-vitals": "^1.1.2",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4"
  },
  "main": "main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "package-mac": "npm run build && electron-builder build --mac --x64 --publish never",
    "package-win": "npm run build && electron-builder build --win --x64 --publish never",
    "package-linux": "npm run build && electron-builder build --linux --x64 --publish never"
  },
  "build": {
    "appId": "io.capstone.ludendorff",
    "productName": "Ludendorff",
    "dmg": {
      "contents": [
        {
          "x": 100,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "type": "distribution",
      "category": "public.app-category.utilities",
      "target": [
        "pkg"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "assets/**/*",
      "build/**/*",
      "main.js",
      "package.json"
    ],
    "extends": null
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.2.1",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.5",
    "electron-builder-squirrel-windows": "^22.14.10",
    "electron-icon-builder": "^2.0.1",
    "electron-packager": "^15.4.0",
    "process": "^0.11.10",
    "wait-on": "^6.0.0"
  }
}
