{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": [
      "./node_modules/user-agent-data-types"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./typings",
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/localization.js",
    "src/shared/const.js",
    "**/node_modules",
    "**/.*/"
  ]
}
